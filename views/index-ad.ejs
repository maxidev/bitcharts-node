<script type="text/javascript">
   $(function() {
       setTimeout(function(){
         $('.chart').easyPieChart({
            easing: 'easeOutCirc',
            animate: 3000,
            delay: 750,
            barColor: '#607B8B',
            trackColor: 'rgba(96, 123, 139, 0.15)',
            scaleColor: '#65a7d2',
            lineCap: 'butt',
            lineWidth: 25,
            size: 191,
         });
      }, 1800);
   });
</script>

